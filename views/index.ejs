        <% for (let i = 0; i < days.length; i++) { %>
        <tr>
            <th>
                <%= days[i].day%>
            </th>
        </tr>
        <tr>
            <td>
                <%= days[i].bodyPart%>
            </td>
        </tr>
        <% } %>
    </table> -->
</body>
</html>